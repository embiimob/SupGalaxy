{
  "name": "supgalaxy",
  "version": "1.0.0",
  "description": "A Fully Decentralized Voxel Adventure where every chunk can be owned, modified and anchored on the bitcoin blockchain with chunk data loaded via IPFS",
  "main": "index.html",
  "scripts": {
    "lint": "eslint js/*.js",
    "lint:strict": "eslint js/*.js --max-warnings 0",
    "lint:fix": "eslint js/*.js --fix",
    "format": "prettier --write \"js/**/*.js\" \"css/**/*.css\" \"*.html\"",
    "format:check": "prettier --check \"js/**/*.js\" \"css/**/*.css\" \"*.html\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/embiimob/SupGalaxy.git"
  },
  "keywords": [
    "voxel",
    "game",
    "blockchain",
    "bitcoin",
    "ipfs",
    "decentralized",
    "threejs"
  ],
  "author": "embiimob",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  }
}
